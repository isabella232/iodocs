{
  "endpoints": [
    {
      "name": "Items",
      "methods": [
        {
          "MethodName": "Item Info",
          "Synopsis": "Returns data for the specified item ID.",
          "HTTPMethod": "GET",
          "URI": "/item/:item_id",
          "RequiresOAuth": "N",
          "parameters": [
            {
              "Name": "item_id",
              "Required": "Y",
              "Default": "",
              "Type": "string",
              "Description": "The ID of the item. Same as the ID found in the URL, e.g. http://rollbar.com/item/:item_id"
            }
          ],
          "examples": {
              "curl": "item.get.curl",
              "python": "item.get.python"
          }
        },
        {
          "MethodName": "List Items",
          "Synopsis": "Returns a page of all items for the project.",
          "HTTPMethod": "GET",
          "URI": "/items/",
          "RequiresOAuth": "N",
          "parameters": [
            { 
              "Name": "status",
              "Required": "N",
              "Default": "",
              "Type": "enumerated",
              "EnumeratedList": [
                "active",
                "resolved"
              ],
              "Description": "If not empty, only return items with the specified status. Note: does not affect the result when 'ids' is used."
            },
            {
              "Name": "ids",
              "Required": "N",
              "Default": "",
              "Type": "comma-separated list of integers",
              "Description": "If not empty, list of item IDs to return instead of using all items from the project."
            },
            {
              "Name": "page",
              "Required": "N",
              "Default": "",
              "Type": "integer",
              "Description": "Page number, optional, starting from 1. 20 items are returned per page."
            }
          ],
          "examples": {
            "curl": "items.get.curl",
            "python": "items.get.python"
          }
        }
      ]
    },
